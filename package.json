{
  "scripts": {
    "postinstall": "cat node_modules/typedoc-plugin-markdown/dist/theme/helpers/parameter-table.js | sed 's/\\\\\\\\|/\\&#124;/g' > /tmp/parameter-table.js && mv /tmp/parameter-table.js node_modules/typedoc-plugin-markdown/dist/theme/helpers/parameter-table.js",
    "start": "cd website && yarn && yarn start",
    "pdf-lib:clone": "git clone git@github.com:Hopding/pdf-lib.git",
    "pdf-lib:pull": "cd pdf-lib && git pull",
    "docs": "rm -rf ./docs/api && cd pdf-lib && git checkout Rewrite && ../node_modules/typedoc/bin/typedoc --options ../typedoc.json",
    "release": "cd website && GIT_USER=Hopding CURRENT_BRANCH=master USE_SSH=true yarn publish-gh-pages"
  },
  "devDependencies": {
    "docusaurus-init": "^1.11.1",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^2.0.10"
  }
}
